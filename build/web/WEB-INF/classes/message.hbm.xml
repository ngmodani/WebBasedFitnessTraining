<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  
    <class name="edu.neu.ngmodani.pojo.Message" table="message">
        <id name="messageID" type="java.lang.Integer">
            <column name="messageID" />
            <generator class="identity" />
        </id>
        <property name="userName" column="userame" type="string" />
        <property name="fromUser" column="fromuser" type="string" />
        <property name="messagebody" column="messagebody" type="string" />
        <property name="messageDate" column="messagedate" type="date" />
        
        
    </class>
  
</hibernate-mapping>
